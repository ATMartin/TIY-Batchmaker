</div>

<div class="slider public-recipes">
</div>

<div class="slider favorite-recipes">
</div>

<div class="slider all-recipes">
  <h4>All Recipes:</h4>
  {{#each recipe in model}}
    <div class="recipe-square" style="border:1px solid red">
      <img {{bind-attr src="recipe.photoUrl"}} style="width: 100px; height: 100px;">
      <button {{action "gotoView" recipe}}> + </button>
    </div>
  {{/each}}
</div>

{{outlet}}
